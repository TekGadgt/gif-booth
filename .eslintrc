{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "global-require": 1,
    "react/prop-types": 1,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "react/jsx-props-no-spreading": 1,
    "no-param-reassign": 1
  }
}
